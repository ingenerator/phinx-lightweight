{
    "name": "ingenerator/phinx-lightweight",
    "type": "library",
    "description": "Very simple SQL-based database migrations tool - a heavily stripped down fork of robmorgan/phinx",
    "keywords": [
        "phinx",
        "migrations",
        "database",
        "db",
        "database migrations"
    ],
    "homepage": "https://github.com/ingenerator/phinx-lightweight",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Coulton",
            "email": "andrew@ingenerator.com",
            "role": "Developer"
        },
        {
            "name": "Craig Gosman",
            "email": "craig@ingenerator.com",
            "role": "Developer"
        },
        {
            "name": "Rob Morgan",
            "email": "robbym@gmail.com",
            "homepage": "https://robmorgan.id.au",
            "role": "Original Lead Developer"
        },
        {
            "name": "Woody Gilk",
            "email": "woody.gilk@gmail.com",
            "homepage": "http://shadowhand.me",
            "role": "Original Developer"
        },
        {
            "name": "Richard Quadling",
            "email": "rquadling@gmail.com",
            "role": "Original Developer"
        },
        {
            "name": "CakePHP Community",
            "homepage": "https://github.com/cakephp/phinx/graphs/contributors",
            "role": "Original contributors"
        }
    ],
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
        "symfony/console": "^4.0",
        "symfony/config": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.5"
    },
    "conflict": {
        "robmorgan/phinx": "*"
    },
    "autoload": {
        "psr-4": {
            "Phinx\\": "src/Phinx"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Phinx\\": "tests/Phinx"
        }
    },
    "bin": [
        "bin/phinx"
    ]
}
